public class Address {
    public static void main(String[] args) {
        // i) Using single print
        System.out.print("Anil, ARS Residency, Greater Noida\n");

        // ii) Using multiple println
        System.out.println("Anil");
        System.out.println("ARS Residency");
        System.out.println("Greater Noida");
    }
}
