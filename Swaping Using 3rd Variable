public class SwapWithoutTemp {
    public static void main(String[] args) {
        int a = 5, b = 10;
        int c;

        System.out.println("Before swap: a = " + a + ", b = " + b);

        c = a;
        a = b;
        b = c;

        System.out.println("After swap: a = " + a + ", b = " + b);
    }
}
